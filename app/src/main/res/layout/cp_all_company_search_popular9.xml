<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cp_all_company_search_popular9_cl"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <ImageView
        android:id="@+id/cp_all_company_search_popular9_logo"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_launcher_background"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <View
        android:id="@+id/cp_all_company_search_popular9_line"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#dfdfdf"
        app:layout_constraintTop_toBottomOf="@id/cp_all_company_search_popular9_logo"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/cp_all_company_search_popular9_name"
        app:layout_constraintLeft_toRightOf="@id/cp_all_company_search_popular9_logo"
        android:text="안녕안녕"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <ImageView
        android:id="@+id/cp_all_company_search_popular9_star"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleX="0.5"
        android:scaleY="0.5"
        android:src="@drawable/cp_p_star"
        app:layout_constraintTop_toBottomOf="@+id/cp_all_company_search_popular9_name"
        app:layout_constraintLeft_toRightOf="@id/cp_all_company_search_popular9_logo"/>
    <TextView
        android:id="@+id/cp_all_company_search_popular9_rate"
        android:text="0.5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/cp_all_company_search_popular9_star"
        app:layout_constraintTop_toBottomOf="@id/cp_all_company_search_popular9_name"/>
    <ImageView
        android:id="@+id/cp_all_company_search_popular9_move"
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        android:src="@drawable/cp_p_right_arrow"
        app:layout_constraintTop_toTopOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>